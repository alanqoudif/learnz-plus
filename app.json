      "favicon": "./assets/favicon.png"
    },
    "fonts": [
      "./assets/fonts/TheYearofHandicrafts-Regular.otf",
      "./assets/fonts/TheYearofHandicrafts-Medium.otf",
      "./assets/fonts/TheYearofHandicrafts-SemiBold.otf",
      "./assets/fonts/TheYearofHandicrafts-Bold.otf",
      "./assets/fonts/TheYearofHandicrafts-Black.otf"
